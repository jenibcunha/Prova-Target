# Define a string a ser invertida
string = "prova da Target"

# Cria uma lista vazia para armazenar os caracteres invertidos
inverted_string = []

# Percorre a string de trÃ¡s para frente e adiciona cada caractere na lista
for i in range(len(string)-1, -1, -1):
    inverted_string.append(string[i])

# Transforma a lista de caracteres em uma string novamente
inverted_string = ''.join(inverted_string)

# Imprime a string invertida
print(inverted_string)
